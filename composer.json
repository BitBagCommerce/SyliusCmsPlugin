{
    "name": "bitbag/cms-plugin",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "sylius-cms"
    ],
    "description": "CMS plugin for Sylius applications.",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "league/csv": "^9.1",
        "friendsofsymfony/ckeditor-bundle": "^1.1|^2.0",
        "sylius/sylius": "^1.7"
    },
    "require-dev": {
        "behat/behat": "^3.6",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "lchrusciel/api-test-case": "^3.0",
        "matthiasnoback/symfony-config-test": "^3.0",
        "phpspec/phpspec": "^6.0",
        "phpstan/phpstan-doctrine": "^0.10",
        "phpstan/phpstan-shim": "^0.10",
        "phpstan/phpstan-symfony": "^0.10",
        "phpstan/phpstan-webmozart-assert": "^0.10",
        "phpunit/phpunit": "^6.5",
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^3.0",
        "symfony/browser-kit": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/intl": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/web-server-bundle": "^4.4"
    },
    "suggest": {
        "stefandoorn/sitemap-plugin": "Adds CMS pages & sections support to the Sylius sitemap plugin"
    },
    "conflict": {
        "symfony/symfony": "4.1.8",
        "symfony/browser-kit": "4.1.8",
        "symfony/dependency-injection": "4.1.8",
        "symfony/dom-crawler": "4.1.8",
        "symfony/routing": "4.1.8"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "BitBag\\SyliusCmsPlugin\\": "src/",
            "Tests\\BitBag\\SyliusCmsPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    }
}
