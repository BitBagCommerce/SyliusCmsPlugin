{
    "name": "bitbag/cms-plugin",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "sylius-cms"
    ],
    "description": "CMS plugin for Sylius applications.",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^7.4 || ^8.0",
        "sylius/sylius": "~1.8.0 || ~1.9.0 || ~1.10.0",
        "league/csv": "^9.1",
        "friendsofsymfony/ckeditor-bundle": "^1.1||^2.0",
        "instaclick/php-webdriver": "^1.4"
    },
    "require-dev": {
        "behat/behat": "^3.7",
        "behat/mink-selenium2-driver": "^1.4",
        "bitbag/coding-standard": "dev-main",
        "dmore/behat-chrome-extension": "^1.3",
        "dmore/chrome-mink-driver": "^2.7",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-debug-extension": "^2.0",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",
        "lchrusciel/api-test-case": "^4.1||^5.0",
        "friendsofsymfony/oauth-server-bundle": "^1.6 || >2.0.0-alpha.0 ^2.0@dev",
        "phpspec/phpspec": "^7.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "0.12.82",
        "phpstan/phpstan-doctrine": "0.12.33",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpstan/phpstan-webmozart-assert": "0.12.12",
        "phpunit/phpunit": "^9.5",
        "sensiolabs/security-checker": "^6.0",
        "sylius-labs/coding-standard": "^4.0",
        "symfony/browser-kit": "^4.4 || ^5.2",
        "symfony/debug-bundle": "^4.4 || ^5.2",
        "symfony/dotenv": "^4.4 || ^5.2",
        "symfony/intl": "^4.4 || ^5.2",
        "symfony/web-profiler-bundle": "^4.4 || ^5.2",
        "vimeo/psalm": "4.7.1"
    },
    "suggest": {
        "stefandoorn/sitemap-plugin": "^2.0@alpha"
    },
    "suggest": {
        "stefandoorn/sitemap-plugin": "Adds CMS pages & sections support to the Sylius sitemap plugin"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "BitBag\\SyliusCmsPlugin\\": "src/",
            "Tests\\BitBag\\SyliusCmsPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\BitBag\\SyliusCmsPlugin\\": "spec/"
        },
        "classmap": ["tests/Application/Kernel.php"]
    }
}
